    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.1",
    "@cypress/vue": "^3.0.5",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.1",
    "cypress": "^9.1.1",
    "start-server-and-test": "^1.14.0",
    "vite": "^2.7.1"
  }
}
